{% extends 'base.html.twig' %}

{% block body %}
<h1>{{device.name}}</h1>
<div class="row">
	<div class="span3">
	<h3>Screenshot</h3>
	<img src="{{app.system.protocol}}://{{app.system.path}}/devices/img/{{device.id}}" width="208px" height="117px" />
	</div>
	<div class="span3">
		Online: {% if device.online == 1%}Yes{%else %}No%{endif %}
	</div>
</div>
<pre>
{{dump(device)}}
</pre>
{% endblock %}